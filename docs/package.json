{
  "name": "reactn-docs",
  "version": "2.0.0",
  "private": true,
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "eslintConfig": {
    "extends": "react-app"
  },
  "scripts": {
    "build": "yarn symlink && react-scripts build",
    "eject": "react-scripts eject",
    "start": "yarn symlink && react-scripts start",
    "symlink": "[ ! -d \"./node_modules/reactn\" ] && yarn symlink-force || exit 0",
    "symlink-force": "ln -s ../../build ./node_modules/reactn",
    "test": "yarn symlink && react-scripts test"
  },
  "dependencies": {
    "@types/jest": "24.0.11",
    "@types/node": "11.13.2",
    "@types/react": "16.8.13",
    "@types/react-dom": "16.8.3",
    "node-sass": "^4.11.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "2.1.8",
    "typescript": "3.4.3"
  }
}
